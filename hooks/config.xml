	<platform name="ios">
        <icon src="www/res/icon/ios/icon-57.png" width="60" height="60" />
        <icon src="www/res/icon/ios/icon-57-2x.png" width="120" height="120" />
        <splash height="960" platform="ios" src="www/res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="www/res/screen/ios/Default~iphone.png" width="320" />
    </platform>
  
    <preference name="DisallowOverscroll" value="true" />
	<preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
	
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-device" spec="~1.1.5" />
    <plugin name="cordova-plugin-appavailability" spec="~0.4.2" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
